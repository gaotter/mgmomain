<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Mgmo.Main.WebApp</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Mgmo.Main.WebApp.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div>
            <h1>Mgmo home page</h1>
        </div>
        <nav class="navbar">
            <ul>
                <li>
                    <a class="navbar-brand" asp-area="" asp-page="/Index">Home</a>
                </li>
                <li>
                    <a class="navbar-brand" asp-area="" asp-page="/Articles">Articels</a>
                </li>
            </ul>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Mgmo.Main.WebApp - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>